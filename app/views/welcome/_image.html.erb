<% 15.times do |image| %>


<div class="row">
  <div class="col s12 m7">
    <div class="card">
      <div class="card-image">
        <%= image_tag("/assets/#{image}.jpg", class: "image") %>
        <span class="card-title text-black">Main Colors</span>
      </div>
      <div class="card-content">
        <% @hex_codes.each do |hex| %>
          <div class="swatch" style="background-color:#<%= hex[:hex] %>">
            <div>

            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
<% end %>
