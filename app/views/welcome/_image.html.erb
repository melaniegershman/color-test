<% @images.each do |image| %>
  
  <div class="row">
    <div class="col s12 m4">
      <div class="card">
        <div class="card-image">
          <%= image_tag("#{image[:filename]}", class: "image") %>
          <span class="card-title black-text white">Main Colors for <%="#{image[:path]}"%></span>
        </div>
        <div class="card-content">
          <% image[:colors].each do |hex| %>
            <div class="swatch" style="background-color:#<%= hex %>">
              <div>

              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
<% end %>
